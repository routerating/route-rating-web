{
  "name": "@routerating/requests",
  "version": "0.0.4",
  "description": "Requests package for Route Rating.",
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "format": "yarn lint --fix",
    "lint": "eslint src/ --ext .ts --ignore-path ../../.gitignore . ",
    "test": "jest",
    "test:cov": "jest --coverage",
    "watch": "rollup -cw",
    "clean": "find . -name *.log -delete && rm -rf dist coverage"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "input": "src/index.ts",
  "main": "src/cjs/index.js",
  "types": "dist/index.d.ts",
  "cjs": "dist/cjs/index.js",
  "esm": "dist/es/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "dependencies": {
    "@routerating/entities": "^0.0.2",
    "eslint": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@lukeshay/eslint-config-typescript": "^1.0.2",
    "@lukeshay/typescript-config": "^1.0.0",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-strip": "^1.3.3",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2",
    "@lukeshay/rollup-config": "~0.0.3"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "route-rating"
  ]
}
