{
  "name": "@routerating/web",
  "description": "Stencil App Starter",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "stencil build --prerender --log --stats",
    "generate": "stencil generate",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "format": "yarn lint --fix",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:cov": "yarn test --coverage",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "clean": "find . -name *.log -delete && rm -rf dist coverage www loader",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@stencil/core": "^1.15.0",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/store": "^1.3.0",
    "@types/puppeteer": "3.0.1",
    "puppeteer": "4.0.1",
    "workbox-build": "5.1.3"
  }
}
