{
  "name": "@routerating/web",
  "description": "Stencil App Starter",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "private": true,
  "scripts": {
    "build": "stencil build --prerender --log --stats",
    "format": "yarn lint --fix",
    "generate": "stencil generate",
    "lint": "eslint src/ --ext .ts --ignore-path ../../.gitignore",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "clean": "find . -name *.log -delete && rm -rf dist coverage www loader"
  },
  "devDependencies": {
    "@lukeshay/eslint-config-typescript": "^1.0.3",
    "@stencil/core": "^1.12.2",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.1",
    "@stencil/store": "^1.3.0",
    "@types/jest": "^26.0.0",
    "@types/puppeteer": "2.0.1",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "puppeteer": "2.1.1",
    "workbox-build": "4.3.1"
  }
}
